{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "properties": {
        "assigned_at": {
            "type": "string",
            "format": "date-time"
        },
        "evolution_id": {
            "type": "string"
        },
        "id": {
            "type": "string"
        },
        "coordinates": {
            "additionalProperties": false,
            "properties": {
                "latitude": {
                    "type": "number"
                },
                "longitude": {
                    "type": "number"
                }
            },
            "required": [
                "latitude",
                "longitude"
            ],
            "type": "object"
        },
        "met_at": {
            "additionalProperties": false,
            "properties": {
                "moment": {
                    "type": "string",
                    "format": "date-time"
                },
                "timeline_side": {
                    "type": "string",
                    "enum": [
                        "exactly",
                        "sooner",
                        "later",
                        "sooner or later"
                    ]
                },
                "approximation": {
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "required": [
                "moment",
                "timeline_side",
                "approximation"
            ],
            "type": "object"
        }
    },
    "required": [
        "coordinates",
        "met_at",
        "assigned_at",
        "id",
        "evolution_id"
    ],
    "type": "object"
}