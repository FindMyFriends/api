{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": [
                "string",
                "null"
            ]
        },
        "demand_id": {
            "type": "string"
        },
        "evolution_id": {
            "type": [
                "string",
                "null"
            ]
        },
        "seeker_id": {
            "type": [
                "integer",
                "null"
            ]
        },
        "position": {
            "type": [
                "integer",
                "null"
            ]
        },
        "new": {
            "type": [
                "boolean",
                "null"
            ]
        },
        "searched_at": {
            "type": "string",
            "format": "date-time"
        },
        "related_at": {
            "type": [
                "string",
                "null"
            ],
            "format": "date-time"
        },
        "is_correct": {
            "type": [
                "boolean",
                "null"
            ]
        },
        "is_exposed": {
            "type": [
                "boolean",
                "null"
            ]
        },
        "ownership": {
            "type": [
                "string"
            ],
            "enum": [
                "theirs",
                "yours"
            ]
        }
    },
    "required": [
        "id",
        "demand_id",
        "evolution_id",
        "seeker_id",
        "position",
        "new",
        "searched_at",
        "related_at",
        "is_correct",
        "is_exposed",
        "ownership"
    ],
    "type": "object"
}