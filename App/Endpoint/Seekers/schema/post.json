{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "properties": {
        "password": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "general": {
            "additionalProperties": false,
            "properties": {
                "firstname": {
                    "type": [
                        "string"
                    ]
                },
                "sex": {
                    "type": "string",
                    "enum": [
                        "man",
                        "woman"
                    ]
                },
                "lastname": {
                    "type": [
                        "string"
                    ]
                },
                "ethnic_group_id": {
                    "type": "integer"
                },
                "birth_year": {
                    "type": "integer",
                    "minimum": 1888,
                    "maximum": 2003
                }
            },
            "required": [
                "lastname",
                "firstname",
                "ethnic_group_id",
                "sex",
                "birth_year"
            ],
            "type": "object"
        },
        "contact": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "facebook",
                "instagram",
                "phone_number"
            ],
            "properties": {
                "facebook": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "instagram": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "phone_number": {
                    "type": [
                        "string",
                        "null"
                    ]
                }
            }
        }
    },
    "required": [
        "email",
        "general",
        "contact",
        "password"
    ],
    "type": "object"
}